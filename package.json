{
  "name": "object-expressions-js",
  "version": "1.0.1",
  "description": "Evaluates if each condition (in relation to an object) contained in an expression is true, allowing business rules to be defined using expressions.",
  "author": "jgarnett",
  "license": "MIT",
  "keywords": [
    "rules",
    "engine",
    "logic",
    "condition",
    "expression",
    "object",
    "expressions",
    "domain",
    "language"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgarnet/object-expressions-js.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup --dts --minify --clean --format cjs src/index.ts",
    "test": "jest",
    "dev": "tsup --outDir build --clean src/dev-evaluate.ts",
    "evaluate": "sh ./scripts/evaluate.sh"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.12",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.7.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "luxon": "^3.5.0"
  }
}
